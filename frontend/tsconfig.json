{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fitness-app/exercise/data": ["libs/exercise/data/src/index.ts"],
      "@fitness-app/shared/models": ["libs/shared/models/src/index.ts"],
      "@fitness-app/exercise/feature/feature-exercise-list": [
        "libs/exercise/feature/feature-exercise-list/src/index.ts"
      ],
      "@fitness-app/exercise/feature/feature-exercise-edit": [
        "libs/exercise/feature/feature-exercise-edit/src/index.ts"
      ],
      "@fitness-app/shared/ui/confirm-dialog": [
        "libs/shared/ui/confirm-dialog/src/index.ts"
      ],
      "@fitness-app/exercise/ui/exercise-configuration": [
        "libs/exercise/ui/exercise-configuration/src/index.ts"
      ],
      "@fitness-app/exercise/feature/feature-exercise-add": [
        "libs/exercise/feature/feature-exercise-add/src/index.ts"
      ],
      "@fitness-app/calendar/feature/calendar-view": [
        "libs/calendar/feature/calendar-view/src/index.ts"
      ],
      "@fitness-app/calendar/data/calendar-data": [
        "libs/calendar/data/calendar-data/src/index.ts"
      ],
      "@fitness-app/calendar/ui/training-day-configuration": [
        "libs/calendar/ui/training-day-configuration/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
